<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res/kms7530.notier"
    android:id="@+id/RelativeLayout1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".TagPickerDialog" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/tag_User"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/btn_tag_non"
            android:gravity="center"
            android:orientation="vertical" >

            <com.beardedhen.androidbootstrap.FontAwesomeText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="60sp"
                app:fa_icon="fa-user" >
            </com.beardedhen.androidbootstrap.FontAwesomeText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tag_Check"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/btn_tag_non"
            android:gravity="center" >

            <com.beardedhen.androidbootstrap.FontAwesomeText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="60sp"
                app:fa_icon="fa-check" >

            </com.beardedhen.androidbootstrap.FontAwesomeText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tag_Home"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/btn_tag_non"
            android:gravity="center" >

            <com.beardedhen.androidbootstrap.FontAwesomeText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="60sp"
                app:fa_icon="fa-home" >
            </com.beardedhen.androidbootstrap.FontAwesomeText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tag_Briefcase"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="@drawable/btn_tag_non"
            android:gravity="center" >

            <com.beardedhen.androidbootstrap.FontAwesomeText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="60sp"
                app:fa_icon="fa-briefcase" >
            </com.beardedhen.androidbootstrap.FontAwesomeText>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true" >

        <LinearLayout
            android:id="@+id/tag_Coffee"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/btn_tag_non"
            android:gravity="center"
            android:orientation="vertical" >

            <com.beardedhen.androidbootstrap.FontAwesomeText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="60sp"
                app:fa_icon="fa-coffee" >
            </com.beardedhen.androidbootstrap.FontAwesomeText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tag_Book"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/btn_tag_non"
            android:gravity="center" >

            <com.beardedhen.androidbootstrap.FontAwesomeText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="60sp"
                app:fa_icon="fa-book" >

            </com.beardedhen.androidbootstrap.FontAwesomeText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tag_gamepad"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/btn_tag_non"
            android:gravity="center" >

            <com.beardedhen.androidbootstrap.FontAwesomeText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="60sp"
                app:fa_icon="fa-gamepad" >
            </com.beardedhen.androidbootstrap.FontAwesomeText>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>